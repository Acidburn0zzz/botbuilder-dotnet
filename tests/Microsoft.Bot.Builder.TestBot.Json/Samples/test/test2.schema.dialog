{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "Name": {
            "type": "string",
            "$mappings": [
                "personName"
            ]
        },
        "Age": {
            "type": "object",
            "$mappings": [
                "age:Age",
                "age"
            ],
            "required": [
                "number",
                "units"
            ],
            "properties": {
                "number": {
                    "type": "number"
                },
                "units": {
                    "type": "string"
                }
            }
        },
        "Ordinal": {
            "type": "string",
            "$mappings": [
                "ordinal"
            ]
        },
        "Location": {
            "type": "object",
            "$mappings": [
                "geographyV2:Location",
                "geographyV2"
            ],
            "required": [
                "type",
                "location"
            ],
            "properties": {
                "type": {
                    "type": "type"
                },
                "location": {
                    "type": "location"
                }
            }
        },
        "KeyPhrase": {
            "type": "string",
            "$mappings": [
                "keyPhrase"
            ]
        },
        "Money": {
            "type": "object",
            "$mappings": [
                "money:Money",
                "money"
            ],
            "required": [
                "number",
                "units"
            ],
            "properties": {
                "number": {
                    "type": "number"
                },
                "units": {
                    "type": "string"
                }
            }
        },
        "Percentage": {
            "type": "string",
            "$mappings": [
                "percentage"
            ]
        },
        "Temperature": {
            "type": "object",
            "$mappings": [
                "temperature:Temperature",
                "temperature"
            ],
            "required": [
                "number",
                "units"
            ],
            "properties": {
                "number": {
                    "type": "number"
                },
                "units": {
                    "type": "string"
                }
            }
        },
        "Url": {
            "type": "string",
            "$mappings": [
                "url"
            ]
        },
        "Quantity": {
            "type": "number",
            "$mappings": [
                "number"
            ],
            "minimum": 1,
            "maximum": 10
        },
        "Directions": {
            "type": "string",
            "$mappings": [
                "utterance"
            ],
            "$comment": "This indicates the whole string is what maps to this."
        },
        "Length": {
            "type": "object",
            "$mappings": [
                "dimension:Length",
                "dimension"
            ],
            "required": [
                "number",
                "units"
            ],
            "properties": {
                "number": {
                    "type": "number"
                },
                "units": {
                    "type": "string"
                }
            }
        },
        "Bread": {
            "type": "string",
            "enum": [
                "multiGrainWheat",
                "rye",
                "white",
                "wholeWheat"
            ]
        },
        "Cheese": {
            "type": "string",
            "enum": [
                "american",
                "cheddar",
                "feta",
                "gouda",
                "pepper jack",
                "provolone",
                "swiss",
                "none"
            ]
        },
        "HomeStreet": {
            "type": "string",
            "$mappings": [
                "utterance"
            ]
        },
        "HomeZip": {
            "type": "string",
            "pattern": "^[0-9]{5}(?:-[0-9]{4})?$",
            "$mappings": [
                "utterance",
                "number"
            ]
        },
        "Email": {
            "type": "string",
            "$mappings": [
                "email"
            ]
        },
        "CancelConfirmation": {
            "type": "string",
            "$mappings": [
                "Confirmation"
            ]
        },
        "CompleteConfirmation": {
            "type": "string",
            "$mappings": [
                "Confirmation",
                "PROPERTYName"
            ]
        },
        "PropertyToChange": {
            "type": "string",
            "enum": "properties",
            "$mappings": [
                "PROPERTYName"
            ]
        }
    },
    "required": [
        "Name",
        "Age",
        "Ordinal",
        "Location",
        "KeyPhrase",
        "Money",
        "Percentage",
        "Temperature",
        "Url",
        "Quantity",
        "Directions",
        "Length",
        "Bread",
        "Cheese",
        "HomeStreet",
        "HomeZip",
        "Email"
    ],
    "$expectedOnly": [
        "CancelConfirmation",
        "CompleteConfirmation",
        "PropertyToChange"
    ],
    "$public": [
        "Name",
        "Age",
        "Ordinal",
        "Location",
        "KeyPhrase",
        "Money",
        "Percentage",
        "Temperature",
        "Url",
        "Quantity",
        "Directions",
        "Length",
        "Bread",
        "Cheese",
        "HomeStreet",
        "HomeZip",
        "Email"
    ],
    "definitions": {
        "PROPERTYName": {
            "type": "string",
            "enum": "properties",
            "$mappings": [
                "PROPERTYName"
            ]
        }
    }
}